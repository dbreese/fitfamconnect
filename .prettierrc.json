{
    "$schema": "https://json.schemastore.org/prettierrc",
    "singleQuote": true,
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "semi": true,
    "vueIndentScriptAndStyle": false,
    "bracketSameLine": false,
    "proseWrap": "always",
    "arrowParens": "always"
}
